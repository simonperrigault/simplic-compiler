    public void FuncDecl.prettyPrint(PrintStream out, String ind) {
        out.print("int ");
        getFuncName().prettyPrint(out, ind);
        out.println("(");
        for (FuncDeclParam param : getParamListList()) {
            out.print(ind+"    ");
            param.prettyPrint(out, ind+"    ");
            out.println();
        }
        out.println(")");
        getBlock().prettyPrint(out, ind);
    }
    public void FuncDeclParam.prettyPrint(PrintStream out, String ind) {
        out.print("int ");
        getParamName().prettyPrint(out, ind);
    }
